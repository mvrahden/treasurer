{
  "name": "treasurer",
  "version": "1.4.19",
  "description": "Read and write 2d data sets in form of *.json, *.csv or *.tsv... e.g. time series such as historical stock prices.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index"
  },
  "readme": "README.md",
  "scripts": {
    "prepare": "npm run test",
    "test": "./node_modules/.bin/jasmine --config='./jasmine.json'",
    "pretest": "npm run build",
    "build": "./node_modules/.bin/tsc -p .",
    "prebuild": "./node_modules/.bin/rimraf dist",
    "posttest": "./node_modules/.bin/rimraf dist/*.spec.* dist/*.doubles.*"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvrahden/treasurer.git"
  },
  "keywords": [
    "read",
    "write",
    "json",
    "csv",
    "tsv",
    "dependency-free",
    "leightweight",
    "file",
    "file system",
    "lightweight",
    "2d-data",
    "2d-array",
    "array",
    "data",
    "dataset",
    "data set",
    "2dimensional",
    "2d",
    "persistence",
    "serialization"
  ],
  "author": "Menno van Rahden",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/mvrahden/treasurer/issues"
  },
  "homepage": "https://github.com/mvrahden/treasurer#readme",
  "devDependencies": {
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.4.0",
    "jasmine": "^2.9.0",
    "rimraf": "^2.6.2",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "util.promisify": "^1.0.0"
  }
}
